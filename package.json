{
    "name": "presentorbord",
    "version": "1.0.0",
    "main": "out/main/index.js",
    "description": "Educational Whiteboard App",
    "author": "Antigravity",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "electron-vite dev",
        "build": "tsc && electron-vite build",
        "preview": "electron-vite preview",
        "dist": "npm run build && electron-builder"
    },
    "build": {
        "appId": "com.presentorbord.app",
        "productName": "Presentorbord",
        "directories": {
            "output": "dist"
        },
        "files": [
            "out/**/*"
        ],
        "win": {
            "target": [
                "nsis"
            ],
            "icon": "build/icon.ico"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        }
    },
    "dependencies": {
        "@electron-toolkit/preload": "^3.0.2",
        "@tldraw/tldraw": "^2.1.0",
        "clsx": "^2.1.1",
        "jspdf": "^4.1.0",
        "lucide-react": "^0.395.0",
        "pdfjs-dist": "^5.4.624",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwind-merge": "^2.3.0",
        "zustand": "^5.0.11"
    },
    "devDependencies": {
        "@types/pdfjs-dist": "^2.10.377",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.19",
        "electron": "^31.0.2",
        "electron-builder": "^26.8.1",
        "electron-vite": "^2.3.0",
        "png-to-ico": "^3.0.1",
        "postcss": "^8.4.38",
        "tailwindcss": "^3.4.4",
        "typescript": "^5.4.5",
        "vite": "^5.3.1"
    }
}
